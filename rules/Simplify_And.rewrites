// do not recompute bounds 
rewrite(x && true, x),
rewrite(x && false, false),
rewrite(x && x, x),
rewrite((x && y) && x, (x && y)),
rewrite(x && (x && y), (x && y)),
rewrite((x && y) && y, (x && y)),
