rewrite(x <= c1 && c0 <= x, false, c1 < c0),
rewrite(c0 < x && c1 < x, fold(max(c0, c1)) < x),
rewrite(c0 <= x && c1 <= x, fold(max(c0, c1)) <= x),
rewrite(x < c0 && x < c1, x < fold(min(c0, c1))),
rewrite(x <= c0 && x <= c1, x <= fold(min(c0, c1)))


// do recompute bounds
rewrite(broadcast(x, c0) && broadcast(y, c0), broadcast(x && y, c0)),
rewrite((x || (y && z)) && y, (x || z) && y),
rewrite((x || (z && y)) && y, (x || z) && y),
